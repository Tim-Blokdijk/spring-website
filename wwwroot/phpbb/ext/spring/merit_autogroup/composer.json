	"version": "0.1.0",
	"time": "2015-06-23",
	"license": "MIT",
	"authors": [{
			"name": "Tim Blokdijk",
			"email": "tim@timblokdijk.nl"
		}],
	"require": {
		"php": ">=5.3.3",
